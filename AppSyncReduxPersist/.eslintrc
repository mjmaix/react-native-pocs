{
    "extends": ["plugin:unicorn/recommended", "airbnb", "plugin:react-native/all", "plugin:flowtype/recommended"],
    "parser": "babel-eslint",
    "plugins": [
        "unicorn",
        "react",
        "react-native",
        "flowtype"
    ],
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
        "react-native/react-native": true
    },
    "rules": {
        "unicorn/filename-case": 0,
        "react/jsx-filename-extension": 0
    }
}